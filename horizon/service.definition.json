{
    "org": "$HZN_ORG_ID",
    "label": "$SERVICE_NAME for $ARCH",
    "description": "A super-simple json_exporter Horizon service",
    "public": false,
    "documentation": "https://github.com/open-horizon/examples/blob/master/edge/services/helloworld/README.md",
    "url": "$SERVICE_NAME",
    "version": "$SERVICE_VERSION",
    "arch": "$ARCH",
    "sharable": "multiple",
    "requiredServices": [],
    "userInput": [
    ],
    "deployment": {
        "services": {
            "$SERVICE_NAME": {
                "image": "${DOCKER_IMAGE_BASE}_$ARCH:$SERVICE_VERSION",
		"privileged": true,
		"ports": [
                    {
                        "HostPort": "7979:7979/tcp",
                        "HostIP": "0.0.0.0"
                    }
                ],
       		 "network": "host"
            }
        }
    }
}
